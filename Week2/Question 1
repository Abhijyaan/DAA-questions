#include<iostream>
#include<algorithm>

using namespace std;

#define max 100

int main(void)
{
    int n,arr[max],i,key,f=1;
    cout<<"Enter the number of elements : ";
    cin>>n;
    cout<<"Enter the elements (in ascending order) : ";
    for(i=0;i<n;i++)
        cin>>arr[i];
    cout<<"Enter the item you are searching for : ";
    cin>>key;
    int low=0,up=n-1,mid;
    
    //binary search
    while(low<=up)
    {
        
        mid = (low+up)/2;
        if(key < arr[mid])
            up = mid-1;
        else if(key > arr[mid])
            low = mid+1;
        else
        {
            cout<<"Present "<<arr[mid]<<endl;
            
            break;
        }
    }
    if(low > up)
    {
        cout<<"Not Present "<<endl;
        f=0;
        
    }
    if(f==1)
    {
        int count=0;
        for(i=mid;i<n;i++)
        {
            if(arr[i]==arr[mid])
            {
                count++;
            }
            else
            {
                break;
            }
        }
        for(i=mid-1;i>=0;i--)
        {
             if(arr[i]==arr[mid])
            {
                count++;
            }
            else
            {
                break;
            }
            
        }
        cout<<count;
    }
    
    return 0;
}
